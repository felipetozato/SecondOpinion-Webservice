{
  "name": "secondopinion-api",
  "version": "0.0.1",
  "description": "Second Opinion API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --inspect\"",
    "clean": "rm -rf dist && mkdir dist",
    "build-babel": "babel -d ./dist ./src -s -D",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node --optimize_for_size --max_old_space_size=460 --gc_interval=100 ./dist",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "jshint": {
    "esversion": 6
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/developit/express-es6-rest-api.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.16.3",
    "morgan": "^1.9.1",
    "quickblox": "^2.12.2",
    "request-promise": "^4.2.2",
    "resource-router-middleware": "^0.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.5.0",
    "nodemon": "^1.18.4"
  },
  "bugs": {
    "url": "https://github.com/developit/express-es6-rest-api/issues"
  },
  "homepage": "https://github.com/developit/express-es6-rest-api#readme",
  "engines": {
    "node": "8.11.4"
  }
}
